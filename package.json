{
    "name": "ng-hateoas-client",
    "version": "2.6.9-0",
    "publishConfig": {
        "registry": "http://registry.npmjs.org/"
    },
    "scripts": {
        "build": "ng build",
        "build:client": "ng build ng-hateoas-client &&  cp README.md dist/ng-hateoas-client/",
        "clean": "rm -rf dist/ *.tgz",
        "lint": "ng lint",
        "ng": "ng",
        "prepublish": "npm run clean && npm run test && npm run build:client && cd dist/ng-hateoas-client && npm pack",
        "serve": "ng serve --port=4422  --proxy-config=./src/environments/proxy.config.json",
        "start": "node src/mock/static-server.js",
        "test": "npm run lint && npm run test:app && npm run test:client",
        "test:app": " ng test hateoas-test-app --watch=false --progress=true",
        "test:client": "ng test  ng-hateoas-client --watch=false --progress=true",
        "test:app:watch": "npm run test:app -- --watch=true --progress=false",
        "test:client:watch": "npm run test:client -- --watch=true --progress=false",
        "test:app:debug": "npm run test:app:watch  -- --browsers=Chrome ",
        "test:client:debug": "npm run test:client:watch --  --browsers=Chrome ",
        "version:patch": "npm version patch && cd projects/ng-hateoas-client && npm version patch --no-git && git commit -a --amend --no-edit  ",
        "version:minor": "npm version minor && cd projects/ng-hateoas-client && npm version minor --no-git && git commit -a --amend --no-edit  ",
        "version:major": "npm version major && cd projects/ng-hateoas-client && npm version major --no-git && git commit -a --amend --no-edit  ",
        "postversion": "git push && git push --tags"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "6.1.8",
        "@angular/common": "6.1.8",
        "@angular/compiler": "6.1.8",
        "@angular/core": "6.1.8",
        "@angular/forms": "6.1.8",
        "@angular/platform-browser": "6.1.8",
        "@angular/platform-browser-dynamic": "6.1.8",
        "@angular/router": "6.1.8",
        "core-js": "^2.5.4",
        "immutable": "3.8.2",
        "rxjs": "6.3.2",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.10.0",
        "@angular-devkit/build-ng-packagr": "^0.10.0",
        "@angular/cli": "~6.2.3",
        "@angular/compiler-cli": "6.1.8",
        "@angular/language-service": "6.1.8",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.3.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^6.3.16",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-story-reporter": "^0.3.1",
        "ng-packagr": "^5.3.0",
        "protractor": "~5.4.0",
        "rxjs-tslint": "^0.1.5",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.29.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "~2.9.2"
    }
}
